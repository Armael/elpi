% a=0, b=0, d=0, w=0, lvl=1>0=gamma
pi x \ sigma X \ pi y \ pi z \ pi w\ ($print "@@" (X z w = Y y w) "@@", X z w  = Y y w, $print "##" (X z w = Y y w) "##").

% a=0, b=0, d=0, w=1, lvl=1>0=gamma
pi x \ sigma X \ pi y \ pi z \ pi w\ ($print "@@" ((a \ X z w a) = Y y w) "@@", (a \ X z w a)  = Y y w, $print "##" ((a \ X z w a) = Y y w) "##").

% a=0, b=0, d=1, w=1, lvl=1>0=gamma
pi x \ sigma X \ pi y \ pi z \ pi w\ ($print "@@" ((c\ a \ X z w a c) = (c\ Y c y w)) "@@", (c \ a \ X z w a c) = (c\ Y c y w), $print "##" ((c\ a \ X z w a c) = (c \ Y c y w)) "##").

